                            <fluent:ScreenTip Title="Lehrkräfte importieren" Text="Wenn diese Option ausgewählt ist, werden Lehrkräfte ins ICC importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Lernende" IsChecked="{Binding ImportStudents, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Lernende importieren" Text="Wenn diese Option ausgewählt ist, werden Lernende ins ICC importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Klassen" IsChecked="{Binding ImportGrades, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Klassen importieren" Text="Wenn diese Option ausgewählt ist, werden Klassen und -zugehörigkeiten ins ICC importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Klassenleitungen" IsChecked="{Binding ImportGradeTeachers, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Klassenleitungen importieren" Text="Wenn diese Option ausgewählt ist, werden Klassenleitungen ins ICC importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Fächer" IsChecked="{Binding ImportSubjects, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Fächer importieren" Text="Wenn diese Option ausgewählt ist, werden Fächer ins ICC importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Lerngruppen" IsChecked="{Binding ImportStudyGroups, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Lerngruppen importieren" Text="Wenn diese Option ausgewählt ist, werden Lerngruppen ins ICC importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Mitgliedschaft in Lerngruppen" IsChecked="{Binding ImportStudyGroupMemberships, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Lerngruppenmitgliedschaften importieren" Text="Wenn diese Option ausgewählt ist, werden Mitgliedschaften in Lerngruppen ins ICC importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Unterrichte" IsChecked="{Binding ImportTuitions, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Unterrichte importieren" Text="Wenn diese Option ausgewählt ist, werden Unterrichte ins ICC importiert."></fluent:ScreenTip>
                        </fluent:CheckBox.ToolTip>
                    </fluent:CheckBox>
                    <fluent:CheckBox Header="Datenschutz" IsChecked="{Binding ImportPrivacy, Mode=TwoWay}">
                        <fluent:CheckBox.ToolTip>
                            <fluent:ScreenTip Title="Datenschutzinformationen importieren" Text="Wenn diese Option ausgewählt ist, werden Datenschutzinformationen ins ICC importiert."></fluent:ScreenTip>
