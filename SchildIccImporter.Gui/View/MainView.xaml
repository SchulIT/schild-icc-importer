                                <Grid Margin="10" HorizontalAlignment="Stretch" DataContext="{Binding Source={StaticResource ViewModelLocator}, Path=Settings}">
                                    <Grid.Resources>
                                        <Style TargetType="TextBlock">
                                            <Setter Property="VerticalAlignment" Value="Center" />
                                        </Style>

                                        <Style TargetType="TextBox">
                                            <Setter Property="Padding" Value="3" />
                                            <Setter Property="Margin" Value="0,3,0,3" />
                                        </Style>

                                        <Style TargetType="ComboBox">
                                            <Setter Property="Padding" Value="5" />
                                            <Setter Property="Margin" Value="0,3,0,3" />
                                        </Style>

                                        <Style TargetType="ComboBoxItem">
                                            <Setter Property="Padding" Value="3" />
                                        </Style>
                                    </Grid.Resources>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <TextBlock Style="{StaticResource HeadingTextBlockStyle}"
                                            Grid.ColumnSpan="2">SchILD-Einstellungen</TextBlock>

                                    <TextBlock Grid.Row="1" Grid.Column="0">Verbindungszeichenfolge</TextBlock>
                                    <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding ConnectionString, UpdateSourceTrigger=PropertyChanged}"></TextBox>

                                    <CheckBox Grid.Row="2" Grid.ColumnSpan="2" Margin="0,5"
                                              Content="Nur sichtbare Elemente exportieren (Fächer, Klassen)"></CheckBox>

                                    <TextBlock Grid.Row="3" Grid.Column="0" VerticalAlignment="Top" TextWrapping="Wrap">Lernende mit Status berücksichtigen</TextBlock>
                                    <ListBox Grid.Row="3" Grid.Column="1"
                                             SelectionMode="Multiple"
                                             ItemsSource="{Binding SchuelerStatusList}">
                                        <b:Interaction.Behaviors>
                                            <behavior:SelectedItemsBehavior SelectedItems="{Binding EnabledSchuelerStatusList}" />
                                        </b:Interaction.Behaviors>
                                    </ListBox>

                                    <TextBlock Style="{StaticResource HeadingTextBlockStyle}"
                                               Grid.Row="4" Grid.ColumnSpan="2">ICC Einstellungen</TextBlock>

                                    <TextBlock Grid.Row="5" Grid.Column="0">URL</TextBlock>
                                    <TextBox Grid.Row="5" Grid.Column="1" Text="{Binding IccEndpoint, UpdateSourceTrigger=PropertyChanged}"></TextBox>

                                    <TextBlock Grid.Row="6" Grid.Column="0">Token</TextBlock>
                                    <TextBox Grid.Row="6" Grid.Column="1" Text="{Binding IccToken, UpdateSourceTrigger=PropertyChanged}"></TextBox>

                                    <fluent:Button Header="Einstellungen speichern" IsDefinitive="False"
